{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:58751",
      "sslPort": 44388
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://localhost:44388",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Alpha": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://localhost:44388",
        "ASPNETCORE_ENVIRONMENT": "Alpha"
      }
    },
    "Production": {
      "commandName": "Project",
      "launchBrowser": true,
      "ASPNETCORE_URLS": "https://localhost:44388"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}
